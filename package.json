{
  "name": "todo-api",
  "version": "1.0.0",
  "description": "A RESTful API built with Express to manage a to-do list",
  "keywords": [
    "api",
    "todo",
    "express",
    "mongodb"
  ],
  "main": "server.js",
  "scripts": {
    "dev": "set PORT=3000 && nodemon --exec babel-node app/server.js",
    "prestart": "babel app --out-dir build",
    "start": "set PORT=8000 && node build/server.js"
  },
  "license": "MIT",
  "private": true,
  "dependencies": {
    "body-parser": "^1.15.0",
    "express": "^4.13.4",
    "mongoose": "^4.4.13"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "nodemon": "^1.10.2"
  }
}
